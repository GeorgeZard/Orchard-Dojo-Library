<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>ocreatecontentpartrecordtable</Title>
      <Shortcut>ocreatecontentpartrecordtable</Shortcut>
      <Description>Code snippet for a SchemaBuilder.CreateTable() call for content part records.</Description>
      <Author>Lombiq</Author>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>name</ID>
          <ToolTip>Name of the content part</ToolTip>
          <Default>MyFirst</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp">
        <![CDATA[SchemaBuilder.CreateTable(typeof($name$PartRecord).Name,
            table => table
                .ContentPartRecord()
                $end$
            );]]>
      </Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>